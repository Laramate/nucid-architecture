{
    "name": "laramate/nucid-architecture",
    "description": "The Nucid Architecture for Laravel",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Laramate",
            "email": "coding@laramate.de",
            "homepage": "https://www.laramate.de/",
            "role": "Team"
        },
        {
            "name": "Tobi Kivelip",
            "email": "tkivelip@gmail.com",
            "homepage": "https://github.com/tkivelip",
